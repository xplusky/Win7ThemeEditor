<Page
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity" xmlns:ic="clr-namespace:Microsoft.Expression.Interactivity.Core;assembly=Microsoft.Expression.Interactions"
	x:Class="ThemeInstaller.Pages.LicenceSence"
	x:Name="MainPageElement"
	WindowTitle="Page"
	FlowDirection="LeftToRight"
	Width="664" Height="417" Loaded="MainPageElement_Loaded">

	<Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>
        <Button x:Name="BtnAgree" Content="{DynamicResource BtnAgree}" Height="40" VerticalAlignment="Center" IsDefault="True" Click="BtnAgree_Click" Template="{DynamicResource ButtonControlTemp2}" Width="150" HorizontalAlignment="Center" Grid.Row="1" Margin="200,0,0,0" />
		<TextBox x:Name="TextboxLicense" Margin="15,10" TextWrapping="Wrap" Template="{DynamicResource TextBoxControlTemplate1}" FontSize="16" VerticalScrollBarVisibility="Auto" IsReadOnly="True"/>
		<Button x:Name="BtnDisagree" Content="{DynamicResource BtnDisagree}" Height="40" VerticalAlignment="Center" IsDefault="True" Click="BtnDisagree_Click" Template="{DynamicResource ButtonControlTemp2}" Width="150" HorizontalAlignment="Center" Grid.Row="1" Margin="-200,0,0,0" />
	</Grid>
</Page>